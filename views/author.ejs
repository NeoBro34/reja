                <div class="left_1">
                    <div class="left_1-1">
                        <img src="images/profile.png" 
                        width="156px"
                        height="142px"
                        alt="">
                    </div>
                    <div class="left_1-2"><%= user.name %></div>
                    <div class="left_1-3"><%= user.profession %></div>
                    <div class="left-icons">
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/facebook.svg" alt="">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/instagram.svg" alt="">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/twitter.svg" alt="">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/youtube.svg" alt="">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/linkedin.svg" alt="">
                            </div>
                        </a>
                        <a class="link_href" href="">
                            <div class="icon">
                                <img class="icon-1" src="icon/dribbble.svg" alt="">
                            </div>
                        </a>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left_2">
                    <div class="left_2-1">
                        <div class="left_2-key">Age:</div>
                        <div class="left_2-value"><%= user.age %></div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-key">Residence:</div>
                        <div class="left_2-value"><%= user.residence %></div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-key">Freelance:</div>
                        <div class="left_2-value" style="color: #7EB942;">Available</div>
                    </div>
                    <div class="left_2-1">
                        <div class="left_2-key">Address</div>
                        <div class="left_2-value"><%= user.adress %></div>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left_3">
                    <h1 class="left_3-lang">Language</h1>
